<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/button_green" />
            <stroke
                android:width="3dp"
                android:color="@color/button_green" />
            <padding
                android:left="3dp"
                android:top="3dp"
                android:right="3dp"
                android:bottom="3dp" />
        </shape>
    </item>

    <item
        android:state_checked="false"
        android:drawable="@drawable/item_checkmark"/>
</layer-list>
